gtk-theme-name="{{gtk.widget-theme}}"
gtk-icon-theme-name="{{gtk.icon-theme}}"
gtk-font-name="{{font-sans}} {{gtk.font-size}}"
gtk-cursor-theme-name="{{cursor-theme}}"
gtk-cursor-theme-size={{cursor-size}}
gtk-toolbar-style=GTK_TOOLBAR_BOTH
gtk-toolbar-icon-size=GTK_ICON_SIZE_LARGE_TOOLBAR
gtk-button-images=0
gtk-menu-images=1
gtk-enable-event-sounds=1
gtk-enable-input-feedback-sounds=1
gtk-xft-antialias=1
gtk-xft-hinting=1
gtk-xft-hintstyle="{{font.hint-style}}"
gtk-xft-rgba="rgb"
